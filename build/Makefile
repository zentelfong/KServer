.NOTPARALLEL:

ifndef config
  config=release_linux
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_win32)
  KServer_config = debug_win32
endif
ifeq ($(config),debug_win64)
  KServer_config = debug_win64
endif
ifeq ($(config),debug_linux)
  KServer_config = debug_linux
endif
ifeq ($(config),release_win32)
  KServer_config = release_win32
endif
ifeq ($(config),release_win64)
  KServer_config = release_win64
endif
ifeq ($(config),release_linux)
  KServer_config = release_linux
endif

PROJECTS := KServer

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

KServer:
ifneq (,$(KServer_config))
	@echo "==== Building KServer ($(KServer_config)) ===="
	@${MAKE} --no-print-directory -C . -f KServer.make config=$(KServer_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f KServer.make clean

help:
